<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="uit.qass.model">
    <class name="Publication" table="dblp_pub_new">
        <id column="id" name="id" type="integer">
            <generator class="native"/>
        </id>
        <property column="dblp_key" name="dblp_key" type="string"/>
        <property column="title" name="title" type="string"/>
        <property column="source" name="source" type="string"/>
        <property column="source_id" name="source_id" type="string"/>
        <property column="series" name="series" type="string"/>
        <property column="year" name="year" type="integer"/>
        <property column="type" name="type" type="string"/>
        <property column="volume" name="volume" type="string"/>
        <property column="number" name="number" type="string"/>
        <property column="month" name="month" type="string"/>
        <property column="pages" name="pages" type="string"/>
        <property column="ee" name="ee" type="string"/>
        <property column="ee_PDF" name="ee_PDF" type="string"/>
        <property column="url" name="url" type="string"/>
        <property column="publisher" name="publisher" type="string"/>
        <property column="isbn" name="isbn" type="string"/>
        <property column="crossref" name="crossref" type="string"/>
        <property column="titleSignature" name="titleSignature" type="string"/>
        <property column="doi" name="doi" type="string"/>
        <property column="mdate" name="mdate" type="date"/>
        <bag name="refPubs" table="dblp_ref_new" cascade="all">
            <key column="id" />
            <many-to-many column="dblp_key" unique="true" class="Publication"  property-ref="dblp_key"/>
        </bag>
        <bag name="authors" table="dblp_author_ref_new" cascade="all">
            <key column="pub_id"/>
            <one-to-many class="Author" />
        </bag>
    </class>
</hibernate-mapping>
